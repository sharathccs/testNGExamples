<?xml version="1.0" encoding="UTF-8"?>
<suite name="Listener Suite" verbose="1">
	<listeners>
		<listener class-name="listener.ListenerClass" />
	</listeners>

	<test name="TestNG logs sample" preserve-order="true">
		<classes>
			<class name="listener.TestClassForListener">
				<methods>
					<include name="methodAddingNumbers" />
					<include name="dividedByZero" />
					<include name="methodSkip" />
				</methods>
			</class>
		</classes>
	</test>
</suite>